openapi: 3.0.3
info:
  title: Banking API
  version: 1.0.0
  description: Comprehensive Banking API for managing accounts, transactions, loans,
    and operations
paths:
  /users/auth/login/:
    post:
      operationId: users_auth_login_create
      description: Authenticate user with email and password to obtain JWT tokens.
        Available to all valid user accounts. Progressive rate limiting with delays
        and account lockouts applied.
      summary: User Login
      tags:
      - Authentication
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                email:
                  type: string
                  format: email
                  example: user@example.com
                  description: User email address
                password:
                  type: string
                  format: password
                  example: securepassword123
              required:
              - email
              - password
      security:
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          content:
            application/json:
              schema:
                type: object
                properties:
                  refresh:
                    type: string
                    example: eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9...
                  access:
                    type: string
                    example: eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9...
                  user:
                    type: object
                    properties:
                      id:
                        type: string
                        example: 123e4567-e89b-12d3-a456-426614174000
                      email:
                        type: string
                        format: email
                        example: spper@example.com
                      first_name:
                        type: string
                        example: John
                      last_name:
                        type: string
                        example: Doe
                      role:
                        type: string
                        enum:
                        - customer
                        - cashier
                        - mobile_banker
                        - manager
                        - operations_manager
                        example: customer
                      is_active:
                        type: boolean
                        example: true
                      is_staff:
                        type: boolean
                        example: false
          description: ''
        '400':
          content:
            application/json:
              schema:
                type: object
                properties:
                  detail:
                    type: string
                    example: No active account found with the given credentials
          description: ''
        '429':
          content:
            application/json:
              schema:
                type: object
                properties:
                  detail:
                    type: string
                    example: Account temporarily locked due to multiple failed login
                      attempts. Try again in 900 seconds.
          description: ''
  /users/auth/refresh/:
    post:
      operationId: users_auth_refresh_create
      description: |-
        Takes a refresh type JSON web token and returns an access type JSON web
        token if the refresh token is valid.
      tags:
      - users
      requestBody:
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/TokenRefreshRequest'
          application/x-www-form-urlencoded:
            schema:
              $ref: '#/components/schemas/TokenRefreshRequest'
          multipart/form-data:
            schema:
              $ref: '#/components/schemas/TokenRefreshRequest'
        required: true
      security:
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/TokenRefresh'
          description: ''
  /users/auth/logout/:
    post:
      operationId: users_auth_logout_create
      description: Invalidate the refresh token to log out the authenticated user.
        Rate limiting applied to prevent abuse.
      summary: User Logout
      tags:
      - Authentication
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                refresh:
                  type: string
                  example: eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9...
              required:
              - refresh
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          content:
            application/json:
              schema:
                type: object
                properties:
                  detail:
                    type: string
                    example: Successfully logged out.
          description: ''
        '400':
          content:
            application/json:
              schema:
                type: object
                properties:
                  detail:
                    type: string
                    example: Invalid token.
          description: ''
        '401':
          content:
            application/json:
              schema:
                type: object
                properties:
                  detail:
                    type: string
                    example: Authentication credentials were not provided.
          description: ''
        '429':
          content:
            application/json:
              schema:
                type: object
                properties:
                  detail:
                    type: string
                    example: Transaction operations temporarily suspended due to excessive
                      activity. Try again in 1800 seconds.
          description: ''
  /users/auth/password-reset/:
    post:
      operationId: users_auth_password_reset_create
      description: 'POST: Request password reset by sending email with reset token.'
      tags:
      - users
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      - {}
      responses:
        '200':
          description: No response body
  /users/auth/password-reset-confirm/:
    post:
      operationId: users_auth_password_reset_confirm_create
      description: 'POST: Confirm password reset with token.'
      tags:
      - users
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      - {}
      responses:
        '200':
          description: No response body
  /users/profile/:
    get:
      operationId: users_profile_retrieve
      description: |-
        GET: Retrieve profile.
        PATCH: Update general profile details.
      tags:
      - users
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          description: No response body
    patch:
      operationId: users_profile_partial_update
      description: |-
        GET: Retrieve profile.
        PATCH: Update general profile details.
      tags:
      - users
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          description: No response body
  /users/notifications/:
    patch:
      operationId: users_notifications_partial_update
      description: 'PATCH: Update notification preferences (notify_email, notify_sms,
        notify_push).'
      tags:
      - users
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          description: No response body
  /users/change-password/:
    post:
      operationId: users_change_password_create
      description: 'POST: Change user password.'
      tags:
      - users
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          description: No response body
  /users/auth/check/:
    get:
      operationId: users_auth_check_retrieve
      description: Returns the authenticated user's information if logged in. Rate
        limiting applied to prevent abuse.
      summary: Check Authentication Status
      tags:
      - Authentication
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          content:
            application/json:
              schema:
                type: object
                properties:
                  authenticated:
                    type: boolean
                    example: true
                  user:
                    type: object
                    properties:
                      id:
                        type: string
                        example: 123e4567-e89b-12d3-a456-426614174000
                      email:
                        type: string
                        format: email
                        example: user@example.com
                      first_name:
                        type: string
                        example: John
                      last_name:
                        type: string
                        example: Doe
                      role:
                        type: string
                        enum:
                        - customer
                        - cashier
                        - mobile_banker
                        - manager
                        - operations_manager
                        example: customer
                      is_active:
                        type: boolean
                        example: true
                      is_staff:
                        type: boolean
                        example: false
          description: ''
        '401':
          content:
            application/json:
              schema:
                type: object
                properties:
                  detail:
                    type: string
                    example: Authentication credentials were not provided.
          description: ''
        '429':
          content:
            application/json:
              schema:
                type: object
                properties:
                  detail:
                    type: string
                    example: Transaction operations temporarily suspended due to excessive
                      activity. Try again in 1800 seconds.
          description: ''
  /users/auth/register/:
    post:
      operationId: users_auth_register_create
      description: Create a new user account with email, password, and basic information.
        Rate limiting applied to prevent abuse.
      summary: User Registration
      tags:
      - Authentication
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                email:
                  type: string
                  format: email
                  example: newuser@example.com
                first_name:
                  type: string
                  example: John
                last_name:
                  type: string
                  example: Doe
                password:
                  type: string
                  format: password
                  example: securepassword123
                password_confirm:
                  type: string
                  format: password
                  example: securepassword123
              required:
              - email
              - first_name
              - last_name
              - password
              - password_confirm
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      - {}
      responses:
        '201':
          content:
            application/json:
              schema:
                type: object
                properties:
                  detail:
                    type: string
                    example: User registered successfully.
                  user:
                    type: object
                    properties:
                      id:
                        type: string
                        example: 123e4567-e89b-12d3-a456-426614174000
                      email:
                        type: string
                        format: email
                        example: newuser@example.com
                      first_name:
                        type: string
                        example: John
                      last_name:
                        type: string
                        example: Doe
                      role:
                        type: string
                        example: customer
          description: ''
        '400':
          content:
            application/json:
              schema:
                type: object
                properties:
                  detail:
                    type: string
                    example: Validation error details
          description: ''
        '429':
          content:
            application/json:
              schema:
                type: object
                properties:
                  detail:
                    type: string
                    example: Transaction operations temporarily suspended due to excessive
                      activity. Try again in 1800 seconds.
          description: ''
  /users/member-dashboard/:
    get:
      operationId: users_member_dashboard_retrieve
      description: Returns dashboard data including account balances, recent transactions,
        and loan information for authenticated members.
      summary: Member Dashboard
      tags:
      - Dashboard
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/RoleBasedDashboard'
          description: ''
        '401':
          content:
            application/json:
              schema:
                type: object
                properties:
                  detail:
                    type: string
                    example: Authentication credentials were not provided.
          description: ''
        '403':
          content:
            application/json:
              schema:
                type: object
                properties:
                  detail:
                    type: string
                    example: Access denied. Members only.
          description: ''
  /users/service-requests/:
    get:
      operationId: users_service_requests_retrieve
      description: Get all service requests for the authenticated user.
      summary: List Service Requests
      tags:
      - Service Requests
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          content:
            application/json:
              schema:
                type: array
                items:
                  type: object
                  properties:
                    id:
                      type: string
                    request_type:
                      type: string
                    description:
                      type: string
                    delivery_method:
                      type: string
                    status:
                      type: string
                    created_at:
                      type: string
          description: ''
    post:
      operationId: users_service_requests_create
      description: Create a new service request for the authenticated user.
      summary: Create Service Request
      tags:
      - Service Requests
      requestBody:
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/ServiceRequestRequest'
          application/x-www-form-urlencoded:
            schema:
              $ref: '#/components/schemas/ServiceRequestRequest'
          multipart/form-data:
            schema:
              $ref: '#/components/schemas/ServiceRequestRequest'
        required: true
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '201':
          content:
            application/json:
              schema:
                type: object
                properties:
                  message:
                    type: string
                    example: Service request created successfully
                  request_id:
                    type: string
                    example: req_123456789
          description: ''
  /users/enable-2fa/:
    post:
      operationId: users_enable_2fa_create
      description: Enable two-factor authentication for the authenticated user.
      summary: Enable 2FA
      tags:
      - Security
      requestBody:
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/Enable2FARequest'
          application/x-www-form-urlencoded:
            schema:
              $ref: '#/components/schemas/Enable2FARequest'
          multipart/form-data:
            schema:
              $ref: '#/components/schemas/Enable2FARequest'
        required: true
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          content:
            application/json:
              schema:
                type: object
                properties:
                  message:
                    type: string
                    example: Two-factor authentication enabled successfully
          description: ''
  /users/send-otp/:
    post:
      operationId: users_send_otp_create
      description: Send OTP code to phone number. In test mode, OTP is returned in
        response for testing.
      summary: Send OTP
      tags:
      - Authentication
      requestBody:
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/OTPRequestRequest'
          application/x-www-form-urlencoded:
            schema:
              $ref: '#/components/schemas/OTPRequestRequest'
          multipart/form-data:
            schema:
              $ref: '#/components/schemas/OTPRequestRequest'
        required: true
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      - {}
      responses:
        '200':
          content:
            application/json:
              schema:
                type: object
                properties:
                  message:
                    type: string
                    example: OTP sent successfully
                  expires_in:
                    type: integer
                    example: 300
                  test_mode:
                    type: boolean
                    example: true
                  otp_code:
                    type: string
                    example: '123456'
                    description: Only in test mode
          description: ''
  /users/verify-otp/:
    post:
      operationId: users_verify_otp_create
      description: Verify OTP code for phone number verification.
      summary: Verify OTP
      tags:
      - Authentication
      requestBody:
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/OTPVerifyRequest'
          application/x-www-form-urlencoded:
            schema:
              $ref: '#/components/schemas/OTPVerifyRequest'
          multipart/form-data:
            schema:
              $ref: '#/components/schemas/OTPVerifyRequest'
        required: true
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      - {}
      responses:
        '200':
          content:
            application/json:
              schema:
                type: object
                properties:
                  message:
                    type: string
                    example: Phone number verified successfully
                  verified:
                    type: boolean
                    example: true
          description: ''
        '400':
          content:
            application/json:
              schema:
                type: object
                properties:
                  error:
                    type: string
                    example: Invalid or expired OTP
          description: ''
  /users/create/:
    post:
      operationId: users_create_create
      description: Create a new user account. Only accessible by managers and operations
        managers.
      summary: Create New User
      tags:
      - User Management
      requestBody:
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/UserCreationRequest'
          application/x-www-form-urlencoded:
            schema:
              $ref: '#/components/schemas/UserCreationRequest'
          multipart/form-data:
            schema:
              $ref: '#/components/schemas/UserCreationRequest'
        required: true
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '201':
          content:
            application/json:
              schema:
                type: object
                properties:
                  message:
                    type: string
                    example: User created successfully
                  user:
                    type: object
                    properties:
                      id:
                        type: string
                      email:
                        type: string
                      first_name:
                        type: string
                      last_name:
                        type: string
                      role:
                        type: string
          description: ''
        '403':
          content:
            application/json:
              schema:
                type: object
                properties:
                  error:
                    type: string
                    example: Only managers and operations managers can create users
          description: ''
  /users/staff/register/:
    post:
      operationId: users_staff_register_create
      description: Create a new staff member with complete profile, banking details,
        and required documents. Only accessible by managers and operations managers.
      summary: Enhanced Staff Registration
      tags:
      - Staff Registration
      requestBody:
        content:
          multipart/form-data:
            schema:
              type: object
              properties:
                email:
                  type: string
                  format: email
                first_name:
                  type: string
                last_name:
                  type: string
                phone:
                  type: string
                role:
                  type: string
                  enum:
                  - cashier
                  - mobile_banker
                  - manager
                  - operations_manager
                  - administrator
                password:
                  type: string
                  format: password
                house_address:
                  type: string
                contact_address:
                  type: string
                government_id:
                  type: string
                ssnit_number:
                  type: string
                bank_name:
                  type: string
                account_number:
                  type: string
                branch_code:
                  type: string
                routing_number:
                  type: string
                passport_picture:
                  type: string
                  format: binary
                application_letter:
                  type: string
                  format: binary
                appointment_letter:
                  type: string
                  format: binary
              required:
              - email
              - first_name
              - last_name
              - phone
              - role
              - password
              - house_address
              - government_id
              - ssnit_number
              - bank_name
              - account_number
              - branch_code
              - routing_number
              - passport_picture
              - application_letter
              - appointment_letter
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '201':
          content:
            application/json:
              schema:
                type: object
                properties:
                  message:
                    type: string
                    example: Staff member registered successfully
                  user:
                    type: object
                    properties:
                      id:
                        type: string
                      email:
                        type: string
                      first_name:
                        type: string
                      last_name:
                        type: string
                      role:
                        type: string
                  documents_created:
                    type: integer
                    example: 3
          description: ''
        '400':
          content:
            application/json:
              schema:
                type: object
                properties:
                  error:
                    type: string
                    example: Validation error details
          description: ''
        '403':
          content:
            application/json:
              schema:
                type: object
                properties:
                  error:
                    type: string
                    example: Insufficient permissions
          description: ''
  /users/staff/:
    get:
      operationId: users_staff_retrieve
      description: Get a list of all staff members. Only accessible by managers and
        operations managers.
      summary: List Staff Members
      tags:
      - User Management
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          content:
            application/json:
              schema:
                type: array
                items:
                  type: object
                  properties:
                    id:
                      type: string
                    email:
                      type: string
                    first_name:
                      type: string
                    last_name:
                      type: string
                    role:
                      type: string
                    is_active:
                      type: boolean
                    date_joined:
                      type: string
                    staff_id:
                      type: string
          description: ''
  /users/staff-ids/:
    get:
      operationId: users_staff_ids_retrieve
      description: Get a list of all staff IDs with advanced filtering and searching.
        Only accessible by managers and operations managers.
      summary: List Staff IDs
      parameters:
      - in: query
        name: employment_date_from
        schema:
          type: string
          format: date
        description: Filter by employment date from
      - in: query
        name: employment_date_to
        schema:
          type: string
          format: date
        description: Filter by employment date to
      - in: query
        name: page
        schema:
          type: integer
        description: Page number for pagination
      - in: query
        name: page_size
        schema:
          type: integer
        description: Number of items per page
      - in: query
        name: role
        schema:
          type: string
          enum:
          - administrator
          - cashier
          - manager
          - mobile_banker
          - operations_manager
        description: Filter by role
      - in: query
        name: search
        schema:
          type: string
        description: Search by name, email, or staff ID
      - in: query
        name: status
        schema:
          type: string
          enum:
          - active
          - inactive
        description: Filter by status
      tags:
      - Staff Management
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          content:
            application/json:
              schema:
                type: object
                properties:
                  count:
                    type: integer
                    example: 25
                  next:
                    type: string
                    nullable: true
                    example: http://api.example.com/api/users/staff-ids/?page=2
                  previous:
                    type: string
                    nullable: true
                    example: null
                  results:
                    type: array
                    items:
                      type: object
                      properties:
                        id:
                          type: string
                          example: 123e4567-e89b-12d3-a456-426614174000
                        email:
                          type: string
                          format: email
                          example: john.doe@example.com
                        first_name:
                          type: string
                          example: John
                        last_name:
                          type: string
                          example: Doe
                        role:
                          type: string
                          example: cashier
                        staff_id:
                          type: string
                          example: JD0323
                        employment_date:
                          type: string
                          format: date
                          example: '2023-03-15'
                        is_active:
                          type: boolean
                          example: true
                        date_joined:
                          type: string
                          format: date-time
                          example: '2023-03-15T10:30:00Z'
          description: ''
        '403':
          content:
            application/json:
              schema:
                type: object
                properties:
                  error:
                    type: string
                    example: Insufficient permissions to view staff IDs
          description: ''
  /users/members/:
    get:
      operationId: users_members_retrieve
      description: Get a list of all members. Accessible by staff users.
      summary: List Members
      tags:
      - User Management
      security:
      - jwtAuth: []
      - Bearer:
          type: http
          scheme: bearer
          bearerFormat: JWT
      responses:
        '200':
          content:
            application/json:
              schema:
                type: array
                items:
                  type: object
                  properties:
                    id:
                      type: string
                    email:
                      type: string
                    first_name:
                      type: string
                    last_name:
                      type: string
                    phone:
                      type: string
                    is_active:
                      type: boolean
                    date_joined:
                      type: string
          description: ''
  /users/deactivate-staff/:
    post:
      operationId: users_deactivate_staff_create
      description: Deactivate a staff member. Only accessible by operations managers.
      summary: Deactivate Staff
      tags:
      - User Management
      requestBody:
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/DeactivateRequest'
          application/x-www-form-urlencoded:
            schema:
              $ref: '#/components/schemas/DeactivateRequest'
          multipart/form-data:
            schema:
              $ref: '#/components/schemas/DeactivateRequest'
