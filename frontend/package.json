{
  "name": "coastal-frontend",
  "type": "module",
  "scripts": {
    "api:sync": "npx openapi-typescript-codegen --input http://localhost:8000/api/schema/ --output src/api --client tanstack",
    "dev": "npm run api:sync && npx vite",
    "build": "npx vite build",
    "preview": "npx vite preview",
    "analyze": "npx vite-bundle-visualizer dist",
    "test": "npx vitest",
    "precommit": "npm run api:sync && git diff --exit-code src/api"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@sentry/browser": "^10.26.0",
    "@sentry/react": "^10.26.0",
    "@sentry/replay": "^7.116.0",
    "@sentry/tracing": "^7.120.4",
    "@tanstack/react-query": "^5.90.10",
    "@vitejs/plugin-react": "^5.1.1",
    "amplitude-js": "^8.21.9",
    "autoprefixer": "^10.4.21",
    "axios": "^1.13.2",
    "dompurify": "^3.2.7",
    "emoji-picker-react": "^4.16.1",
    "lucide-react": "^0.555.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^6.8.0",
    "recharts": "^3.4.1",
    "serve": "^14.2.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "jsdom": "^24.1.1",
    "openapi-typescript-codegen": "^0.29.0",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^4.0.12"
  }
}
