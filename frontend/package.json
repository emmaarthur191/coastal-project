{
  "name": "coastal-frontend",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "api:sync": "npx openapi-typescript-codegen --input ../banking_backend/schema.yml --output src/api --client axios",
    "dev": "npm run api:sync && npx vite",
    "build": "node ./node_modules/vite/bin/vite.js build",
    "preview": "node ./node_modules/vite/bin/vite.js preview",
    "analyze": "npx vite-bundle-visualizer dist",
    "test": "npx vitest",
    "lint": "npx eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "precommit": "npm run api:sync && git diff --exit-code src/api",
    "postbuild": "echo Build completed with local assets only"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/camera": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/geolocation": "^8.0.0",
    "@headlessui/react": "^2.2.9",
    "@sentry/browser": "^10.26.0",
    "@sentry/react": "^10.26.0",
    "@sentry/replay": "^7.116.0",
    "@sentry/tracing": "^7.120.4",
    "@tanstack/react-query": "^5.90.10",
    "@vitejs/plugin-react": "^5.1.1",
    "amplitude-js": "^8.21.9",
    "autoprefixer": "^10.4.21",
    "axios": "^1.13.2",
    "dompurify": "^3.2.7",
    "emoji-picker-react": "^4.16.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "lucide-react": "^0.555.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^6.8.0",
    "recharts": "^3.4.1",
    "serve": "^14.2.0",
    "tailwindcss": "^3.4.18",
    "terser": "^5.36.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "whatwg-encoding": "^3.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "mkdirp": "^3.0.1",
    "openapi-typescript-codegen": "^0.29.0",
    "rimraf": "^6.1.2",
    "typescript-eslint": "^8.50.1",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^4.0.16"
  }
}
